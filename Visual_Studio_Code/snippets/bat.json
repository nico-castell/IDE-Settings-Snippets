{
	// Place your snippets for bat here. Each snippet is defined under a snippet name and has a prefix, body and
	// description. The prefix is what is used to trigger the snippet and the body will be expanded and inserted. Possible variables are:
	// $1, $2 for tab stops, $0 for the final cursor position, and ${1:label}, ${2:another} for placeholders. Placeholders with the
	// same ids are connected.
	"Start": {
		// The necessary lines to start the script correctly.
		"prefix": "start",
		"body": "@echo off\ntitle $1\n\n$0",
		"description": "The start of a batch scrip.t"
	},
	"Region": {
		// A region inside a batch / bat / cmd / DOS script.
		"prefix": "region",
		"body": "::#region\n\n$0\n\n::#endregion",
		"description": "A batch region."
	}
}
